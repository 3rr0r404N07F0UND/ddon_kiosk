h1
  text-align: center;
  font-size: 30px;
  height: auto;
.printSection
  width: 100%;
  height: 95%;
  overflow-y: auto;
  display: block;
html, body
  width: 100vw;
  height: 100vh;
  overflow: hidden;
ul
  display: grid;
  grid-template-columns: 1fr 10fr 2fr 4fr;
  width: 100%;
  li
    border: 1px solid black;
    padding-left: 5px;
